<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"	xmlns:lio="http://slothsoft.net"	xmlns:exsl="http://exslt.org/common"	xmlns:date="http://exslt.org/dates-and-times"	xmlns:func="http://exslt.org/functions"	xmlns:str="http://exslt.org/strings"	xmlns:php="http://php.net/xsl"	extension-element-prefixes="func">		<xsl:template match="error" mode="cms">		<details open="open" class="errorMessage">			<summary><xsl:value-of select="@message"/></summary>			<pre>				<xsl:value-of select="."/>			</pre>		</details>	</xsl:template> 	<xsl:template match="/data">		<html>			<head><title>Slothsoft - Fantasy Critic</title>			<style type="text/css"><![CDATA[* {	font-family: "Myriad Pro", Myriad, "Liberation Sans", "Nimbus Sans L", "Helvetica Neue", Helvetica, Arial, sans-serif;}html, body {	margin: 0;	padding: 0;}table {	width: 100%;}.game {	font-weight: bold;}.number {	font-family: monospace;	text-align: right;}small {	font-size: 0.6em;}			]]></style>				<xsl:copy-of select="*"/>			</head>						<body>				<xsl:apply-templates select="//error" mode="cms"/>				<xsl:call-template name="table"/>			</body>		</html>	</xsl:template>		<xsl:template name="table">		<table border="1">			<thead>				<tr>					<th>Publisher</th>					<th>Developer</th>					<th>Game</th>					<th>Release Date</th>					<th>Critic Score Link</th>					<th>Hype Factor</th>					<th>Projected Points</th>					<th>% Picked</th>					<th>% Counter Picked</th>					<th>Tags</th>				</tr>			</thead>			<tbody>				<xsl:for-each select="//game">					<xsl:sort select="@hypeFactor" order="descending" data-type="number"/>					<tr>						<td>							<xsl:if test="@publisher">								<a href="{lio:wiki(@publisher)}" target="_blank"><xsl:value-of select="@publisher"/></a>							</xsl:if>						</td>						<td>							<xsl:if test="@developer">								<a href="{lio:wiki(@developer)}" target="_blank"><xsl:value-of select="@developer"/></a>							</xsl:if>						</td>						<td class="title"><a href="{@href-wiki}" target="_blank"><xsl:value-of select="@gameName"/></a></td>						<td title="{@minimumReleaseDate} - {@maximumReleaseDate}"><xsl:value-of select="@estimatedReleaseDate"/></td>						<td class="number">							<xsl:if test="@criticScore">								<a href="{@href-critic}" target="_blank"><xsl:value-of select="format-number(@criticScore, '#0.00')"/>%</a>							</xsl:if>						</td>						<td class="number"><xsl:value-of select="format-number(@hypeFactor, '#0.00')"/>%</td>						<td class="number">							<a href="{@href-fc}" target="_blank">								<xsl:value-of select="format-number(@projectedFantasyPoints, '#0.00')"/>							</a>						</td>						<td class="number"><xsl:value-of select="format-number(@percentStandardGame, '#0.00')"/>%</td>						<td class="number"><xsl:value-of select="format-number(@percentCounterPick, '#0.00')"/>%</td>						<td><small><xsl:value-of select="@readableTags"/></small></td>					</tr>				</xsl:for-each>			</tbody>		</table>	</xsl:template>		<func:function name="lio:parse-duration">		<xsl:param name="string"/>		<xsl:variable name="tokens" select="str:tokenize($string, ':')"/>		<func:result select="$tokens[1] * 60 * 60 + $tokens[2] * 60"/>	</func:function>		<func:function name="lio:format-duration">		<xsl:param name="number"/>		<xsl:variable name="hours" select="floor($number div 3600)"/>		<xsl:variable name="minutes" select="($number - $hours * 3600) div 60"/>		<func:result select="concat(format-number($hours, '00'), ':', format-number($minutes, '00'))"/>	</func:function>		<func:function name="lio:wiki">		<xsl:param name="term" select="."/>		<func:result>			<xsl:text>https://en.wikipedia.org/w/index.php?title=Special:Search&amp;search=</xsl:text>			<xsl:value-of select="php:functionString('urlencode', normalize-space($term))"/>		</func:result>	</func:function></xsl:stylesheet>