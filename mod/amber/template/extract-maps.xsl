<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"	xmlns:save="http://schema.slothsoft.net/savegame/editor"	xmlns:str="http://exslt.org/strings"	xmlns:func="http://exslt.org/functions"	xmlns:exsl="http://exslt.org/common"	extension-element-prefixes="str func exsl">		<xsl:template match="/data/data">		<dictionary dictionary-id="map-ids">			<xsl:for-each select=".//save:file[@file-name &gt; 256]">				<option key="{@file-name}" val="{.//*[@value]/@value}"/>			</xsl:for-each>		</dictionary>	</xsl:template></xsl:stylesheet>